initialize LANCEMENT = false
initialize FIN_PARCOURS = false
initialize POINT_A = false
initialize POINT_B = false
initialize POINT_C = false
initialize speed = 0

// DÃ©finitions des modes

  // Niveau 1

    set MODE = ACTIVE {
      LANCEMENT = true
    } INACTIVE

  // Niveau 2

    set MODE = DEPART {
      MODE = ACTIVE
      POINT_A = false
      POINT_B = false
      POINT_C = false
      FIN_PARCOURS = false
    }

    set MODE = RETOUR {
      MODE = ACTIVE
      FIN_PARCOURS = true
    }

    set MODE = GOPOINT_A {
      MODE = ACTIVE
      POINT_A = true
      POINT_B = false
      POINT_C = false
      FIN_PARCOURS = false
    }

    set MODE = GOPOINT_B {
      MODE = ACTIVE
      POINT_A = false
      POINT_B = true
      POINT_C = false
      FIN_PARCOURS = false
    }

    set MODE = GOPOINT_C {
      MODE = ACTIVE
      POINT_A = false
      POINT_B = false
      POINT_C = true
      FIN_PARCOURS = false
    }

// Configuration des behaviors
// Depart
Behavior = BHV_ConstantHeading
{
  // General Behavior Parameters
  // ---------------------------
  name         = const_hdg
  pwt          = 100
  condition    = MODE== DEPART
  updates      = CONST_HDG_UPDATES
  endflag      = POINT_A = true

  // Parameters specific to this behavior
  // ------------------------------------
             basewidth = 10
              duration = 100
  heading_mismatch_var = HDG_DIFF
             peakwidth = 10
           summitdelta = 25
           heading = 90

}

 Behavior = BHV_ConstantSpeed
 {
   // General Behavior Parameters
   // ---------------------------
   name         = depart_const_spd_transit  // example
   pwt          = 100                // default
   condition    = MODE == DEPART   // example
   updates      = CONST_SPD_UPDATES  // example

   // Parameters specific to this behavior
   // ------------------------------------
            basewidth = 0.2          // default
             duration = 100           // default
                speed = 2            // default
   speed_mismatch_var = SPEED_DIFF   // example
            peakwidth = 0            // default
          summitdelta = 0            // default
 }

